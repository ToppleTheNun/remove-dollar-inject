{
  "name": "remove-dollar-inject",
  "version": "0.0.0-development",
  "description": "A library for deleting any $inject in an Angular project.",
  "main": "index.js",
  "files": [
    "index.js",
    "dist"
  ],
  "repository": "https://github.com/Nunnery/remove-dollar-inject.git",
  "author": "Richard Harrah <topplethenunnery@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=5"
  },
  "dependencies": {
    "jscodeshift": "^0.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@commitlint/travis-cli": "^7.2.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.4.2",
    "babel-plugin-add-module-exports": "^1.0.0",
    "commitizen": "^3.0.4",
    "cz-conventional-changelog": "^2.1.0",
    "documentation": "^8.1.2",
    "eol": "^0.9.1",
    "eslint": "^5.8.0",
    "eslint-config-google": "^0.11.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.8.0",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.10.6"
  },
  "scripts": {
    "build": "babel src -d dist --ignore spec.js",
    "clean": "rimraf dist",
    "commit": "git-cz",
    "coverage": "npm run test -- --coverage",
    "docs": "documentation readme src --section=API",
    "format": "prettier --write src/**/*.js",
    "lint": "eslint src",
    "prebuild": "npm run docs && npm run clean",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "travis-commitlint": "commitlint-travis",
    "semantic-release": "semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm run lint && npm run test"
    }
  }
}
