{
  "name": "remove-dollar-inject",
  "version": "0.0.0-development",
  "description": "A library for deleting any $inject in an Angular project.",
  "main": "index.js",
  "files": [
    "index.js",
    "dist"
  ],
  "repository": "https://github.com/Nunnery/remove-dollar-inject.git",
  "author": "Richard Harrah <topplethenunnery@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "jscodeshift": "^0.6.3"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.3",
    "@babel/preset-env": "7.4.3",
    "@commitlint/cli": "7.5.2",
    "@commitlint/config-conventional": "7.5.0",
    "@commitlint/travis-cli": "7.5.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.1.0",
    "babel-plugin-add-module-exports": "1.0.0",
    "codecov": "3.3.0",
    "commitizen": "3.0.7",
    "cz-conventional-changelog": "2.1.0",
    "documentation": "9.3.1",
    "eol": "0.9.1",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "3.0.1",
    "husky": "1.3.1",
    "jest": "24.1.0",
    "prettier": "1.16.4",
    "pretty-quick": "1.10.0",
    "rimraf": "2.6.3",
    "semantic-release": "15.13.3"
  },
  "scripts": {
    "build": "babel src -d dist --ignore spec.js",
    "clean": "rimraf dist",
    "codecov": "codecov",
    "commit": "git-cz",
    "docs": "documentation readme src --section=API",
    "format": "prettier --write src/**/*.js",
    "lint": "eslint src",
    "prebuild": "npm run docs && npm run clean",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "travis-commitlint": "commitlint-travis",
    "semantic-release": "semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "./coverage/",
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm run lint && npm run test"
    }
  }
}
